# load local startup stuff. .localrc is not checked in.
if [ -f $HOME/.localrc ]; then
	source $HOME/.localrc
fi;

# load aliases
source $HOME/.aliases

# base16 Shell
BASE16_SHELL="$HOME/.config/base16-shell/"
[ -n "$PS1" ] && \
    [ -s "$BASE16_SHELL/profile_helper.sh" ] && \
        source "$BASE16_SHELL/profile_helper.sh"

base16_default-dark

